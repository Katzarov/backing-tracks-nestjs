{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "youtube-downloader": {
      "type": "application",
      "root": "apps/youtube-downloader",
      "entryFile": "main",
      "sourceRoot": "apps/youtube-downloader/src",
      "compilerOptions": {
        "tsConfigPath": "apps/youtube-downloader/tsconfig.app.json"
      }
    },
    "file-converter": {
      "type": "application",
      "root": "apps/file-converter",
      "entryFile": "main",
      "sourceRoot": "apps/file-converter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-converter/tsconfig.app.json"
      }
    },
    "track-storage": {
      "type": "library",
      "root": "libs/track-storage",
      "entryFile": "index",
      "sourceRoot": "libs/track-storage/src",
      "compilerOptions": {
        "tsConfigPath": "libs/track-storage/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    }
  }
}